{
	"name": "DataSourceFile",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"contenedor": {
				"type": "string"
			},
			"directorio": {
				"type": "string"
			},
			"filename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@{dataset().directorio}",
					"type": "Expression"
				},
				"container": {
					"value": "@{dataset().contenedor}",
					"type": "Expression"
				}
			}
		}
	}
}